From 861a97f05380f840497ab1599037ab9fdd3f405f Mon Sep 17 00:00:00 2001
From: czapek1337 <czapek1337@gmail.com>
Date: Sat, 21 May 2022 21:49:46 +0200
Subject: [PATCH] Add zigux as a target

---
 bfd/config.bfd              | 5 +++++
 config.sub                  | 2 +-
 gas/configure.tgt           | 1 +
 ld/configure.tgt            | 3 +++
 ld/emulparams/elf_x86_64.sh | 2 +-
 libiberty/rust-demangle.c   | 4 ++++
 6 files changed, 15 insertions(+), 2 deletions(-)

diff --git a/bfd/config.bfd b/bfd/config.bfd
index cfe58247..4e920d36 100644
--- a/bfd/config.bfd
+++ b/bfd/config.bfd
@@ -706,6 +706,11 @@ case "${targ}" in
     targ_selvecs="i386_elf32_vec iamcu_elf32_vec x86_64_elf32_vec i386_pei_vec x86_64_pe_vec x86_64_pei_vec l1om_elf64_vec k1om_elf64_vec"
     want64=true
     ;;
+  x86_64-*-zigux*)
+    targ_defvec=x86_64_elf64_vec
+    targ_selvecs=i386_elf32_vec
+    want64=true
+    ;;
   x86_64-*-mingw* | x86_64-*-pe | x86_64-*-pep | x86_64-*-cygwin)
     targ_defvec=x86_64_pe_vec
     targ_selvecs="x86_64_pe_vec x86_64_pei_vec x86_64_pe_big_vec x86_64_elf64_vec l1om_elf64_vec k1om_elf64_vec i386_pe_vec i386_pei_vec i386_elf32_vec iamcu_elf32_vec"
diff --git a/config.sub b/config.sub
index dba16e84..855fe457 100755
--- a/config.sub
+++ b/config.sub
@@ -1728,7 +1728,7 @@ case $os in
 	     | hpux* | unos* | osf* | luna* | dgux* | auroraux* | solaris* \
 	     | sym* |  plan9* | psp* | sim* | xray* | os68k* | v88r* \
 	     | hiux* | abug | nacl* | netware* | windows* \
-	     | os9* | macos* | osx* | ios* \
+	     | os9* | macos* | osx* | ios* | zigux* \
 	     | mpw* | magic* | mmixware* | mon960* | lnews* \
 	     | amigaos* | amigados* | msdos* | newsos* | unicos* | aof* \
 	     | aos* | aros* | cloudabi* | sortix* | twizzler* \
diff --git a/gas/configure.tgt b/gas/configure.tgt
index 62f806bd..b4b658a3 100644
--- a/gas/configure.tgt
+++ b/gas/configure.tgt
@@ -225,6 +225,7 @@ case ${generic_target} in
   i386-*-beos*)				fmt=elf ;;
   i386-*-elfiamcu)			fmt=elf arch=iamcu ;;
   i386-*-elf*)				fmt=elf ;;
+  i386-*-zigux*)			fmt=elf ;;
   i386-*-fuchsia*)			fmt=elf ;;
   i386-*-haiku*)			fmt=elf em=haiku ;;
   i386-*-genode*)			fmt=elf ;;
diff --git a/ld/configure.tgt b/ld/configure.tgt
index c7acf3f1..8de51c47 100644
--- a/ld/configure.tgt
+++ b/ld/configure.tgt
@@ -967,6 +967,9 @@ x86_64-*-linux-*)	targ_emul=elf_x86_64
 			targ_extra_libpath="elf_i386 elf32_x86_64 elf_l1om elf_k1om"
 			tdir_elf_i386=`echo ${targ_alias} | sed -e 's/x86_64/i386/'`
 			;;
+x86_64-*-zigux*)
+            targ_emul=elf_x86_64
+            ;;
 x86_64-*-redox*)	targ_emul=elf_x86_64
 			targ_extra_emuls=elf_i386
 			;;
diff --git a/ld/emulparams/elf_x86_64.sh b/ld/emulparams/elf_x86_64.sh
index 5f2743ed..f8f99bc8 100644
--- a/ld/emulparams/elf_x86_64.sh
+++ b/ld/emulparams/elf_x86_64.sh
@@ -43,7 +43,7 @@ fi
 # Linux/Solaris modify the default library search path to first include
 # a 64-bit specific directory.
 case "$target" in
-  x86_64*-linux*|i[3-7]86-*-linux-*)
+  x86_64*-linux*|i[3-7]86-*-linux-*|x86_64*-zigux*)
     case "$EMULATION_NAME" in
       *64*)
 	LIBPATH_SUFFIX=64
diff --git a/libiberty/rust-demangle.c b/libiberty/rust-demangle.c
index 3b24d638..8e2789de 100644
--- a/libiberty/rust-demangle.c
+++ b/libiberty/rust-demangle.c
@@ -32,6 +32,10 @@ If not, see <http://www.gnu.org/licenses/>.  */
 #include "config.h"
 #endif
 
+#if defined(__zigux__)
+#define uint unsigned int
+#endif
+
 #include "safe-ctype.h"
 
 #include <inttypes.h>
-- 
2.36.1

